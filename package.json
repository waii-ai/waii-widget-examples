{
  "name": "waii-widget-examples",
  "version": "1.0.0",
  "description": "Example implementations of Waii chat widget - the world's best text-to-SQL API",
  "author": "Waii, Inc.",
  "license": "MIT",
  "workspaces": [
    "iframe",
    "react-examples/chat-with-history",
    "react-examples/chat",
    "react-examples/history",
    "standalone"
  ],
  "scripts": {
    "start:iframe": "cd iframe && npm start",
    "start:react-chat-with-history": "cd react-examples/chat-with-history && npm start",
    "serve:prod:react-chat-with-history": "cd react-examples/chat-with-history && npm run serve:prod",
    "start:react-chat": "cd react-examples/chat && npm start",
    "serve:prod:react-chat": "cd react-examples/chat && npm run serve:prod",
    "start:react-history": "cd react-examples/history && npm start",
    "serve:prod:react-history": "cd react-examples/history && npm run serve:prod",
    "start:standalone": "cd standalone && npm start"
  },
  "dependencies": {
    "@waii-ai/widgets": "1.27.7-beta.8"
  }
}
